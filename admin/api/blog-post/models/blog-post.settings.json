{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "name": "Blog Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Slug": {
      "type": "uid",
      "required": true
    },
    "Title": {
      "type": "string",
      "required": true,
      "maxLength": 64
    },
    "Content": {
      "type": "richtext"
    },
    "Gallery": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    }
  }
}
